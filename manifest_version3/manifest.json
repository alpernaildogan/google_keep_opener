{
  "manifest_version": 3,
  "name": "Google Keep popup!",
  "version": "1.0",
  "description": "Opens the keep.google.com in a popup window",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": ["activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
